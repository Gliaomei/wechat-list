.custom-bar {
    position: relative;
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    border-bottom: 10rpx;
  }
  .custom-bar__wrapper {
    padding: 0 10rpx;
    display: flex;
    justify-content: center;
    align-items: center;
  }
  .search-group {
    width: 80%;
    height: 100%;
    display: flex;
    justify-content: flex-start;
    align-items: center;
    border: 1px solid #ccc;
    border-radius: 60rpx;
    padding: 0 10rpx;
  }
  .search-group > input {
    font-size: 25rpx;
  }
  .search-group > image {
    height: 32rpx;
    width: 32rpx;
    margin-right: 20rpx
  }
  