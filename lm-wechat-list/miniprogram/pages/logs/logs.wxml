<scroll-view class="content-list" scroll-y='true' style="height:{{windowHeight}}px" bindscrolltolower='loadMoreImages'>
  <image 
    wx:for='{{dataList}}'  
    wx:key='item' 
    src='{{item.src}}'
    style='position: absolute; top: {{item.top}}px; left: {{item.left}}px; width: {{imgWidth}}px; height: {{item.height}}px' 
    bindload='loadImage' 
    data-index='{{index}}'
  />
</scroll-view>
